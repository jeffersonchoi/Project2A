	<table>
		<tr> 
			<th> First Name </th>
			<th> Last Name </th>
			<th> Date of Birth: </th>
			<th> Email: </th>

		<tr>
	    <% @users.each do |user| %>
			<tr>
				<td> <%= user.first_name %> </td>  
				<td> <%= user.last_name %> </td>
				<td> <%= user.date_of_birth %> </td>
				<td> <%= user.email %> </td>

			</tr>
	    <% end %>
	</table>